;
; BootROM variables and functions
;

VIRQ	equ	VEC_IRQ_ADDR
VSWI	equ	VEC_SWI_ADDR
VNMI	equ	VEC_NMI_ADDR

END_ADDR	equ	$F0

BOOTROM_BASE	equ	$FC00

F_RESET		equ	BOOTROM_BASE
F_UART_IN	equ	BOOTROM_BASE+3*1
F_UART_OUT	equ	BOOTROM_BASE+3*2
F_UART_PUTS	equ	BOOTROM_BASE+3*3
F_UART_OUTHEX	equ	BOOTROM_BASE+3*4
F_SD_INIT	equ	BOOTROM_BASE+3*5
